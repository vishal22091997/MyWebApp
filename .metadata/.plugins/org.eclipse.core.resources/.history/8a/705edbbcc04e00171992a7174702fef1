import java.util.Arrays;
import java.util.Scanner;
 
 class Triplets {
	public static void main(String djd[]){
		Scanner scan = new Scanner(System.in);
		int t = scan.nextInt();
		while(t-->0){
			int p = scan.nextInt(), q = scan.nextInt(), r = scan.nextInt();
			int a[] = new int[p];
			int b[] = new int[q];
			int c[] = new int[r];
			for(int i=0;i<p;i++){
				a[i] = scan.nextInt();
			}
			 
			for(int j=0;j<q;j++){
				b[j] = scan.nextInt();
			}
			 
			for(int k=0;k<r;k++){
				c[k] = scan.nextInt();
			}
			boolean yCheck[] = new boolean[q];
			long ans = 0;
			
			
			 
			
			
			 
			System.out.println(ans);
		}
	}
 
} 