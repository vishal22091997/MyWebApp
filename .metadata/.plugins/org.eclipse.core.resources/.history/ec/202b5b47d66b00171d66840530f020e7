package Programs;

import java.util.Scanner;

public class MaxinmumGCD {
	public static void main(String jd[]){
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		int a[] = new int[n];
		int b[] = new int[n];
		int max = 0;
		for(int i=0;i<n;i++){
			a[i] = scan.nextInt();
			max = Math.max(max, a[i]);
		}
		for(int i=0;i<n;i++){
			b[i] = scan.nextInt();
			max = Math.max(max, b[i]);
		}
		int sam[] = new int[max+1];
		int count[] = new int[max+1];
		
	}

}
