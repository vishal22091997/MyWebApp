import java.util.Scanner;

public class AGoodSet {
	public static void main(String gj[]){
		Scanner scan = new Scanner(System.in);
		int q = scan.nextInt();
		while(q-->0){
			int n = scan.nextInt();
			int last = 0;
			if(n==1){
				System.out.println(1);
			}else if(n==2){
				System.out.println(1 + " "+2);
			}else{
				System.out.print(1+" "+2);
				last = 4;
				for(int i=0;i<n-2;i++){
					System.out.println(last+1);
					last = 2*last+1;
				}
			}
		}
	}

}
