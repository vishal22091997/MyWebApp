import java.util.Scanner;

public class Triplets {
	public static void main(String djd[]){
		Scanner scan = new Scanner(System.in);
		int t = scan.nextInt();
		while(t-->0){
			int p = scan.nextInt(), q = scan.nextInt(), r = scan.nextInt();
			int a[] = new int[p];
			int b[] = new int[q];
			int c[] = new int[r];
			long ans = 0;
			for(int i=0;i<p;i++){
				long x = a[i];
				for(int j=0;j<q;j++){
					long y = b[j];
					if(x<=y){
						for(int k=0;k<r;k++){
							long z = c[k];
							if(y>=z){
								ans += (x+y)*(y+z);
							}
						}
					}
				}
			}
		}
	}

}
