package sethi.vishal.MyWebApp.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "product")
public class Product {
	@Id
	@Column(name = "id")
	private int id;
	

	@Column(name = "brand")
	private int brand;
	

	@Column(name = "id")
	private int id;
	

	@Column(name = "id")
	private int id;
	

	@Column(name = "id")
	private int id;
	

	@Column(name = "id")
	private int id;
	

	@Column(name = "id")
	private int id;
}
