import java.util.Scanner;

public class AGoodSet {
	public static void main(String gj[]){
		Scanner scan = new Scanner(System.in);
		int q = scan.nextInt();
		while(q-->0){
			int n = scan.nextInt();
			 ;
			if(n==1){
				System.out.println(1);
			}else if(n==2){
				System.out.println(1 + " "+2+" ");
			}else{
				System.out.print(1+" "+2+" "+3+" ");
				int last = 2;
				int slast = 1;
				for(int i=0;i<n-3;i++){
					System.out.print(last+slast+1+" ");
					int temp = last+slast+1;
					slast = last;
					last = temp;
				}
				System.out.println();
			}
		}
	}

}
