package Programs;

import java.util.Scanner;

public class CamelCase {
	public String makeIt(String s){
		String ans = "";
		
	}
	public static void main(String hg[]){
		Scanner scan = new Scanner(System.in);
		int t = scan.nextInt();
		while(t-->0){
			int n = scan.nextInt();
			String s[] = new String[n];
			for(int i=0;i<n;i++){
				s[i] = makeIt(scan.next());
			}
			
		}
	}

}
